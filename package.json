{
    "private": true,
    "browserslist": [
        "extends @wordpress/browserslist-config"
    ],
    "engines": {
        "node": ">=12.14.0"
    },
    "scripts": {
        "build": "NODE_OPTIONS=\"--max-old-space-size=8000\" mix",
        "build:production": " mix --production",
        "start": "mix watch",
        "hot": "mix watch --hot",
        "clear": "wp acorn optimize:clear",
        "test": "npm run lint",
        "lint": "npm run lint:js && npm run lint:css",
        "lint:js": "eslint resources/scripts",
        "lint:css": "stylelint \"resources/**/*.{css,scss,vue}\"",
        "translate": "npm run translate:pot && npm run translate:js",
        "translate:pot": "wp i18n make-pot . ./resources/lang/sage.pot --ignore-domain --include=\"app,resources\"",
        "translate:js": "wp i18n make-json ./resources/lang --pretty-print"
    },
    "devDependencies": {
        "@tailwindcss/jit": "^0.1.18",
        "@tailwindcss/typography": "^0.4.0",
        "@tinypixelco/laravel-mix-wp-blocks": "^1.1.0",
        "@wordpress/babel-preset-default": "^5.0.1",
        "@wordpress/browserslist-config": "^3.0.1",
        "@wordpress/dependency-extraction-webpack-plugin": "^3.0.1",
        "autoprefixer": "^10.4.1",
        "babel-eslint": "^10.1.0",
        "browser-sync": "^2.26.14",
        "browser-sync-webpack-plugin": "^2.3.0",
        "eslint": "^7.20.0",
        "eslint-plugin-import": "^2.22.1",
        "laravel-mix": "^6.0.39",
        "node-sass": "^7.0.1",
        "postcss": "^8.4.5",
        "postcss-import": "^14.0.2",
        "sass-loader": "^12.4.0",
        "stylelint": "^13.10.0",
        "stylelint-config-standard": "^20.0.0",
        "svg-vue3": "^0.2.1",
        "tailwindcss": "^3.0.13",
        "vue-template-compiler": "^2.6.12"
    },
    "dependencies": {
        "@splidejs/splide-extension-grid": "^0.3.20",
        "@splidejs/vue-splide": "^0.3.5",
        "@vue-hero-icons/outline": "^1.7.2",
        "@vuex-orm/core": "^0.36.3",
        "aos": "^2.3.4",
        "apollo-boost": "^0.4.9",
        "apollo-cache-inmemory": "^1.6.6",
        "apollo-client": "^2.6.10",
        "apollo-link": "^1.2.14",
        "apollo-link-http": "^1.5.17",
        "axios": "^0.21.4",
        "crypto-js": "^4.1.1",
        "dateformat": "^5.0.3",
        "es6-promise": "^4.2.8",
        "flowbite": "^1.3.4",
        "graphql": "^16.2.0",
        "graphql-tag": "^2.12.6",
        "gsap": "^3.6.1",
        "jquery": "^3.6.0",
        "laravel-mix-svg-vue": "^0.3.2",
        "lodash": "^4.17.20",
        "popper.js": "^1.16.1",
        "postcss-cli": "^9.1.0",
        "postcss-nested": "^5.0.6",
        "svg-vue": "^0.1.2",
        "vue": "^2.6.14",
        "vue-animejs": "^2.1.1",
        "vue-apollo": "^3.1.0",
        "vue-carousel": "^0.18.0",
        "vue-js-modal": "^2.0.1",
        "vue-loader": "^15.9.6",
        "vue-loading-overlay": "^3.4.2",
        "vue-resource": "^1.5.1",
        "vue-select": "^3.16.0",
        "vue-slick-carousel": "^1.0.6",
        "vue-social-sharing": "^3.0.8",
        "vue2-datepicker": "^3.10.4",
        "vuejs-datepicker": "^1.6.2",
        "vuex": "^3.6.2"
    }
}
